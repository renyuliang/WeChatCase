/* components/init-guide/index.wxss */

.date {
  font-size: 20rpx;
  font-weight: 400;
  color: #666;
  line-height: 20rpx;
  /* height: 100%; */
  margin-left: 40rpx;
  /* margin-top: 8rpx; */
}

.tips {
  text-align: center;
  position: relative;
  padding-bottom: 24rpx;
}

.tipsLine {
  display: inline-block;
  width: 32rpx;
  height: 2rpx;
  background: #d8d8d8;
  margin: 0 16rpx;
}

.name {
  font-size: 24rpx;
  color: #b2b2b2;
  line-height: 32rpx;
}

/* 1.0.1 */

.ml-10 {
  margin-left: 10rpx;
}

.list-box {
  background: #fff;
  box-shadow: 0px 4rpx 8rpx 0px rgba(0, 0, 0, 0.04);
  border-radius: 16rpx;
  margin-bottom: 24rpx;
  padding: 24rpx 24rpx 0;
}

.list-msg:last-child {
  margin-bottom: 0;
}

.list-box .get-time {
  font-size: 20rpx;
  font-weight: 400;
  color: #999;
  line-height: 20rpx;
  margin-bottom: 16rpx;
}

.list-box .title {
  font-size: 34rpx;
  font-weight: 400;
  line-height: 44rpx;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-top: 8rpx;
}

.read {
  color: #7f7f7f;
}

.unread {
  color: #212121;
}

.list-box .language {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  background: rgba(253, 126, 31, 0.1);
  border-radius: 16rpx 0px 16rpx 0px;
  font-size: 24rpx;
  font-weight: 500;
  color: #fd7e1f;
  line-height: 40rpx;
  text-align: center;
}

.list-box .is-read {
  color: #7f7f7f;
}

.list-box .author {
  font-size: 24rpx;
  font-weight: 400;
  color: #333;
  line-height: 32rpx;
  padding: 16rpx 0;
}

.bottom-box {
  border-top: 1rpx dashed #b2b2b2;
  min-height: 72rpx;
}

.bottom-box .gift-words {
  font-size: 24rpx;
  font-weight: 400;
  color: #fd7e1f;
  line-height: 32rpx;
  margin-left: 8rpx;
}

.bottom-box .keywords {
  /* height: 32rpx; */
  line-height: 32rpx;
  border-radius: 4rpx;
  box-sizing: border-box;
  border: 1rpx solid rgba(200, 200, 200, 0.8);
  font-size: 20rpx;
  font-weight: 400;
  color: #7f7f7f;
  padding: 0rpx 8rpx;
  margin-right: 8rpx;
  /* margin-top: 16rpx; */
}

.bottom-box .keywords1 {
  border: 1rpx solid #f7402c;
  color: #f7402c;
  opacity: .8;
}

.bottom-box .keywords2 {
  border: 1rpx solid #1d72ef;
  color: #1d72ef;
  opacity: .8;
}

.bottom-box .keywords3 {
  border: 1rpx solid #fd7e1f;
  color: #fd7e1f;
  opacity: .8;
}

.gift-icon {
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.gift-icon-select {
  width: 36rpx;
  height: 36rpx;
  display: inline-block;
  vertical-align: middle;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.awrad{
  /* margin-top: 16rpx; */
}

.initFlexEnd{
  justify-content: flex-end;
}