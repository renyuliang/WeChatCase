/* pages/index/index.wxss */

.Index {
  position: relative;
  height: 100%;
}

.header {
  /* height: 300rpx; */
  background-image: linear-gradient(#1f95fd, #1f7afd);
}

.headName {
  font-size: 40rpx;
  font-weight: bold;
  color: #fff;
  padding: 0rpx 64rpx;
  margin-bottom: 16rpx;
  line-height: 48rpx;
}

.indexAbsolute {
  position: absolute;
  left: 0;
  right: 0;
}

.scrollBack {
  background: #f5f5f5;
  position: absolute;
  left: 0;
  right: 0;
  top: 152rpx;
  padding-top: 230rpx;
}

.swiper-box {
  position: relative;
  height: 384rpx;
  border-radius: 32rpx;
  z-index: 9;
}

.swiper-img {
  width: 100%;
  padding: 0 16rpx;
}

.fixTips {
  position: fixed;
  left: 0;
  right: 0;
  z-index: 9;
  background: #f5f5f5;
}

.indexTips {
  padding:24rpx 32rpx 32rpx;
  color: #212121;
  font-size: 34rpx;
  font-weight: bold;
}

.iconlist_article {
  display: inline-block;
  margin: 0rpx 16rpx 0 0;
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
  background: url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.newGuide {
  padding: 0 32rpx;
}

.wx-swiper-dots {
  position: relative;
  top: 300rpx;
}

.wx-swiper-dot.wx-swiper-dot {
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
}

.wx-swiper-dot.wx-swiper-dot-active {
  width: 32rpx;
  height: 12rpx;
  border-radius: 12rpx;
}

.init-guide .moreOverFlow {
  color: #212121 !important;
}

.loginSearch {
  width: 344rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: rgba(29, 114, 239, 0.1);
  border-radius: 44rpx;
  margin: 32px auto;
  text-align: center;
  color: #1D72EF;
  font-size: 36rpx;
}

.moreGuide{
  color: #FD7E1F;
  font-size: 28rpx;
  font-weight: normal;
}
.iconlist_right_arrow_orange1,.iconlist_right_arrow_blue1{
  font-size: 24rpx;
}
.iconlist_right_arrow_orange1{
  margin-left: -8rpx;
}
.iconlist_right_arrow_blue1{
  vertical-align: middle;
  margin-left: -10rpx;
}